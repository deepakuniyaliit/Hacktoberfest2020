#include <stdio.h>
int main()
{
    int a,sum=0;
    scanf("%d",&a);
    int b[a],c[121]={0};
    for(int i=0;i<a;i++)
    {
        scanf("%d",&b[i]);
    }
    for(int i=0;i<a;i++)
    {
      c[b[i]]++;
     
    }
    for(int i=0;i<=100;i++)
    {
        if(c[i]>=2)
        {
            sum+=(c[i]/2);
           
        } 
    }
    printf("%d",sum);
}
