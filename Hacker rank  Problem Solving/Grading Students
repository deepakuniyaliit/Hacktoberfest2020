#include<stdio.h>
int main()
{
    int a,j;
    scanf("%d",&a);
    int b[a];
    for(int i=0;i<a;i++)
    {
        scanf("%d",&b[i]);
    }
     for(int i=0;i<a;i++)
    {
         for(j=b[i];j%5!=0;j++);
         if(j<38)
         {
             printf("%d\n",b[i]);
         }
         else {
           if(j%5==0)
           {
               if((j-b[i])<3)
               {
                   printf("%d\n",j);
               }
               else {
               printf("%d\n",b[i]);
               }
           }
         }
    }
   

}
